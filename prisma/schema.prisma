// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Recipe {
  id          String   @id @default(cuid())
  recipe_id   String   @unique
  name        String
  author String?
  author_website String?
  preparation_time String?
  cooking_time String?
  servings String?
  difficulty String?
  calories String?
  fats String?
  saturates String?
  carbs String?
  sugars String?
  fibre String?
  diet String?
  ingredients String?
  instructions String?
  protein String?
  salt String?
  description String?
  url String
  image  String
  dish_type String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @default(now())
}